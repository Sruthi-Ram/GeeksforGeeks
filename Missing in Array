class Solution {

    // Note that the size of the array is n-1
    int missingNumber(int n, int arr[]) {
int expectedSum = n * (n + 1) / 2;
        int actualSum = 0;
        for (int num : arr) {
            actualSum += num;
        }
        int missingNumber = expectedSum - actualSum;
        return missingNumber;
        // Your Code Here
    }
}
