class Solution {
    static int nthFibonacci(int n){
        // code her
        if(n == 1 || n == 2) return 1;
        if(n == 3) return 2;
        int a = 1, b = 1;
        int c = 2;
        for(int i=4; i<=n; i++){
            a = b;
            b = c;
            c = (a+b)%1000000007;
        }
        return c;
    }
    }
