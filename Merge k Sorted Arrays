class Solution
{
    //Function to merge k sorted arrays.
    public static ArrayList<Integer> mergeKArrays(int[][] arr,int K) 
    {
        // Write your code here.
        ArrayList al=new ArrayList();
        for(int i=0;i<K;i++)
        {
            for(int j=0;j<K;j++)
            {
                al.add(arr[i][j]);
            }
        }
        Collections.sort(al);
        return al;
        
    }
}
